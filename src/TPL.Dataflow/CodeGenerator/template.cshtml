using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;
using System.Threading.Tasks.Dataflow;

namespace UserNamespace
{
	class Program
	{
		static void Main(string[] args)
		{
			@foreach (var block in Model.Blocks)
			{
				@Raw("			");
				@Raw(@block.GetDefinition());
				@Raw("\n");
			}

			@foreach (var block in Model.Blocks)
			{
				@foreach (var str in @block.GetAdditionalBlocks())
				{
					@Raw("			");
					@Raw(@str);
					@Raw("\n");
				}	
			}

			@foreach (var block in Model.Blocks)
			{
				@foreach (var str in @block.GetConnections())
				{
					@Raw("			");
					@Raw(@str);
					@Raw("\n");
				}	
			}
		}
	}
}
